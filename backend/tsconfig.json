{
  "compilerOptions": {
  "strict": true,
  "module": "CommonJS",
  "target": "ES6",
  "lib": ["es6", "dom"],
  "sourceMap": true,
  "allowJs": true,
  "jsx": "react",
  "esModuleInterop": true,
  "moduleResolution": "node",
  "noImplicitReturns": true,
  "noImplicitThis": true,
  "noImplicitAny": true,
  "strictNullChecks": true,
  "noUnusedLocals": true,
  "skipLibCheck": true
},
"exclude": ["node_modules"]
}
